{
  "name": "cloudify-ui-components",
  "version": "1.2.1",
  "description": "Cloudify UI React components library",
  "homepage": "https://github.com/cloudify-cosmo/cloudify-ui-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudify-cosmo/cloudify-ui-components.git"
  },
  "license": "Apache-2.0",
  "author": "Cloudify Platform Ltd. <cosmo-admin@cloudify.co>",
  "files": [
    "es"
  ],
  "main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "audit": "npm audit --production",
    "build": "bash scripts/build-components.sh",
    "buildDev": "babel ./src --ignore '**/*.stories.jsx' --copy-files --delete-dir-on-start --out-dir es --watch",
    "lint": "eslint --cache --ignore-path .gitignore --ext js,jsx --max-warnings 10 .",
    "test:jest": "jest test --coverage --maxWorkers=2",
    "test:loki": "loki --diffingEngine gm --chromeTolerance 0.9 --requireReference --reactUri file:./storybook-static",
    "test": "npm run lint && npm run test:jest",
    "storybook": "start-storybook --docs -p 9001",
    "build-storybook": "build-storybook --docs -c .storybook --quiet",
    "publish:major": "bash scripts/publish-version.sh major",
    "publish:minor": "bash scripts/publish-version.sh minor",
    "publish:patch": "bash scripts/publish-version.sh patch"
  },
  "keywords": [
    "react",
    "shared",
    "components",
    "cloudify",
    "ui"
  ],
  "bugs": {
    "url": "https://cloudify.co/slack/"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-react": "^7.8.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.17",
    "@storybook/theming": "^5.3.17",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.9.1",
    "@types/prop-types": "^15.7.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.5.4",
    "lodash": "^4.17.19",
    "loki": "^0.25.1",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "sass-loader": "^8.0.2",
    "semantic-ui-css-offline": "^1.0.3-2.4.1",
    "semantic-ui-react": "^0.88.1",
    "style-loader": "^1.1.3"
  },
  "dependencies": {
    "cloudify-ui-common": "^2.0.0",
    "rc-tree": "^2.1.3",
    "react-color": "^2.17.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datepicker": "1.8.0",
    "react-json-view": "^1.19.1",
    "react-live": "^2.2.1",
    "react-syntax-highlighter": "^11.0.2",
    "react-transition-group": "^4.3.0",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.6.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "semantic-ui-react": "^0.84.0",
    "semantic-ui-css-offline": "^1.0.3-2.4.1"
  },
  "loki": {
    "verboseRenderer": true,
    "chromeSelector": "body > div#root > div > *",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}
